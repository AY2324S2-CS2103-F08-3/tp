<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>


<HBox id="meetingPane" alignment="CENTER_LEFT" minHeight="-Infinity" xmlns="http://javafx.com/javafx/17"
      xmlns:fx="http://javafx.com/fxml/1" spacing="35">
    <padding>
        <Insets top="5" right="5" bottom="5" left="15"/>
    </padding>
    <Label fx:id="id" styleClass="cell_meeting_label" text="\$id"/>
    <Label fx:id="date" styleClass="cell_meeting_label" text="\$date"/>
    <Label styleClass="cell_meeting_label" text=" : "/>
    <Label fx:id="description" styleClass="cell_meeting_label" text="\$description"/>

</HBox>


